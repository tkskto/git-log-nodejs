{
  "root": true,
  "extends": [
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "indent": [2, 2, {"SwitchCase": 1}],
    "multiline-comment-style": 0,
    "@typescript-eslint/no-var-requires": 0,
    "no-console": 0,
    "quotes": [2, "single"]
  }
}

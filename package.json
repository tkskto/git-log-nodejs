    "@types/node": "16.4.1",
    "@typescript-eslint/eslint-plugin": "4.28.4",
    "@typescript-eslint/parser": "4.28.4",
    "eslint": "7.31.0",
    "husky": "7.0.1",
    "jest": "27.0.6",
    "lint-staged": "11.1.0",
    "rollup": "2.53.3",
